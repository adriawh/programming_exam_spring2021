<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ntnu.adriawh.controller.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Search:" />
      <TextField fx:id="searchField" promptText="Search by postal code or post office" />
      <TableView fx:id="tableView">
        <columns>
          <TableColumn fx:id="postalCode" prefWidth="75.0" text="postal code" />
          <TableColumn fx:id="postOffice" prefWidth="75.0" text="post office" />
            <TableColumn fx:id="municipalCode" prefWidth="75.0" text="municipal code" />
            <TableColumn fx:id="municipalityName" prefWidth="75.0" text="municipality name" />
            <TableColumn fx:id="category" prefWidth="75.0" text="category" />
        </columns>
      </TableView>
   </children>
</VBox>
